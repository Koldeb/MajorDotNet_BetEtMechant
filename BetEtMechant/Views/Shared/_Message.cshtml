@if (!string.IsNullOrWhiteSpace(TempData["Message"]?.ToString()))
{
    var message = Newtonsoft.Json.JsonConvert.DeserializeObject<BetEtMechant.Class.Flash>(TempData["Message"]?.ToString());



    <div class="alert alert-@message.TypeMessage.ToString().ToLower()">
        @message.Message
    </div>
}